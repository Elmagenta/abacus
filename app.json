{
  "expo": {
    "name": "firefly3",
    "description": "FireFly 3 client for iOS",
    "slug": "firefly-iii",
    "privacy": "hidden",
    "platforms": [
      "ios"
    ],
    "version": "0.0.4",
    "orientation": "portrait",
    "icon": "./src/images/icon-firefly3.png",
    "splash": {
      "image": "./src/images/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "buildNumber": "0.0.4",
      "bundleIdentifier": "firefly.ios.app"
    },
    "scheme": "fireflyiosapp",
    "githubUrl": "https://github.com/victorbalssa/firefly-iii"
  }
}
